{
  "components/products/tabs": "<div class=\"container 4661726c6579\">\r\n    {{#or product.description product.videos.list.length}}\r\n        <div class=\"accordion-block\">\r\n            <div\r\n                class=\"accordion-navigation toggleLink is-open\"\r\n                role=\"button\"\r\n                data-collapsible=\"#accordion--description\">\r\n                <h5 class=\"accordion-title\">\r\n                    {{lang 'products.description'}}\r\n                </h5>\r\n        \r\n                <div class=\"accordion-navigation-actions\">\r\n                    <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                        <use xlink:href=\"#icon-plus\" />\r\n                    </svg>\r\n                    <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                        <use xlink:href=\"#icon-minus\" />\r\n                    </svg>\r\n                </div>\r\n            </div>\r\n\r\n            <div id=\"accordion--description\" class=\"accordion-content is-open\">\r\n                <div {{#if schema}} itemprop=\"description\"{{/if}}>\r\n                    {{{product.description}}}\r\n                    {{{snippet 'product_description'}}}\r\n                </div>\r\n                \r\n                {{#if product.videos.list.length}}\r\n                    {{> components/products/videos product.videos}}\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    {{/or}}\r\n\r\n    {{#unless theme_settings.show_short_desc}}\r\n        {{#if product.warranty}}\r\n            <div class=\"accordion-block\">\r\n                <div\r\n                    class=\"accordion-navigation toggleLink\"\r\n                    role=\"button\"\r\n                    data-collapsible=\"#accordion--warranty\">\r\n                    <h5 class=\"accordion-title\">\r\n                        Warranty\r\n                    </h5>\r\n    \r\n                    <div class=\"accordion-navigation-actions\">\r\n                        <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                            <use xlink:href=\"#icon-plus\" />\r\n                        </svg>\r\n                        <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                            <use xlink:href=\"#icon-minus\" />\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n    \r\n                <div id=\"accordion--warranty\" class=\"accordion-content\">\r\n                    {{{product.warranty}}}\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n    {{/unless}}\r\n</div>\r\n\r\n",
  "components/products/videos": "<section class=\"videoGallery\" data-video-gallery>\n    <div id=\"videoGallery-content\">\n        <div class=\"videoGallery-main\">\n            <iframe\n                id=\"player\"\n                class=\"lazyload\"\n                type=\"text/html\"\n                width=\"640\"\n                height=\"390\"\n                frameborder=\"0\"\n                webkitAllowFullScreen\n                mozallowfullscreen\n                allowFullScreen\n                data-src=\"//www.youtube.com/embed/{{this.featured.id}}?rel=0\"\n                data-video-player>\n            </iframe>\n        </div>\n        <ul class=\"videoGallery-list\">\n        {{#each this.list}}\n            <li class=\"videoGallery-item\">\n                <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\n                    <div class=\"video-figure\">\n                        <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\n                    </div>\n                    <div class=\"video-body\">\n                        <h5 class=\"video-title\">{{title_long}}</h5>\n                        <p class=\"video-description\">{{description_short}}</p>\n                    </div>\n                </a>\n            </li>\n        {{/each}}\n        </ul>\n    </div>\n\n</section>\n"
}
